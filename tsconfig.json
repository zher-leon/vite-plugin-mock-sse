{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "module": "commonjs",
    "declaration": true,
    "outDir": "./lib/",
    "strict": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "types": ["vite/client", "node"],
    "sourceMap": false,
    "noEmitOnError": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts"],
}
